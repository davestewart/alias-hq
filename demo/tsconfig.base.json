{
  "example": true,
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": "src",
    "paths": {
      "@/*": ["/*"],
      "@packages/*": ["../packages/*"],
      "@classes/*": ["classes/*"],
      "@app/*": ["app/*"],
      "@data/*": ["app/data/*"],
      "@settings": ["app/settings.js"],
      "@services/*": [
        "app/services/*",
        "../packages/services/*"
      ],
      "@views/*": ["app/views/*"],
      "@alias-hq/*": ["../../src/*"]
    }
  }
}
